{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "dingo/api": "2.*",
        "grimzy/laravel-mysql-spatial": "^2.0",
        "illuminate/notifications": "^5.6",
        "illuminate/redis": "^5.6",
        "illuminate/session": "^5.6",
        "jacobcyl/ali-oss-storage": "^2.1",
        "laravel/lumen-framework": "5.6.*",
        "league/flysystem": "^1.0",
        "overtrue/laravel-wechat": "~4.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "react/react": "^0.4.2",
        "riverslei/payment": "^4.1",
        "symfony/event-dispatcher": "~4.0",
        "tymon/jwt-auth": "^0.5.12",
        "vlucas/phpdotenv": "~2.2",
        "zoran-wang/jwt-auth-guard": "0.0.1.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.7",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"

    },
    "autoload": {
        "files": [
            "app/Utils/HttpStatusCode.php",
            "app/Utils/PatternDict.php",
            "app/Utils/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
